  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "engines": {    
    "node": "17.x"
  }, 
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "description": ""
}

